load("@rules_elisp//elisp:elisp_test.bzl", "elisp_test")
load("@rules_elisp//elisp/proto:elisp_proto_library.bzl", "elisp_proto_library")

proto_library(
    name = "my_proto",
    srcs = ["my.proto"],
)

some_rule(name = "module")
