[REPORTS]
output-format=parseable

[DESIGN]
max-locals=25
max-args=10
max-branches=15
max-statements=80
max-attributes=15
min-public-methods=1

[FORMAT]
max-line-length=80

[SIMILARITIES]
ignore-imports=yes
min-similarity-lines=15

[STRING]
check-quote-consistency=yes

# Local Variables:
# mode: conf-unix
# End:
